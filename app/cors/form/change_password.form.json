{
    "oldpwd": {
        "translation": "USER.OLDPWD",
        "type": "password",
        "required": true,
        "validation": {"value": "User.isSamePassword(scope.object['oldpwd'])", "translation": "USER.WRONG_OLD_PWD" }
    },
    "newpwd": {
        "translation": "USER.NEWPWD",
        "type": "password",
        "required": true,
        "validation": {"value": "scope.object['oldpwd'] !== scope.object['newpwd']", "translation": "USER.NEW_PWD_SAME_OLD_PWD" }
    },
    "confpwd": {
        "translation": "USER.CONFPWD",
        "type": "password",
        "required": true,
        "validation": {"value": "scope.object['newpwd'] === scope.object['confpwd']", "translation": "CONF_NOT_SAME_PASSWORD" }
    }
}